extends /Dashboard/Header
block PageContent
  // Begin Page Content
  .container-fluid
    // Page Heading
    .d-sm-flex.align-items-center.justify-content-between.mb-4
      h1.h3.mb-0.text-gray-800 پەڕەی سەرەکی
      |             
      //- a.d-none.d-sm-inline-block.btn.btn-sm.btn-primary.shadow-sm(href='#')
      //-   i.fas.fa-download.fa-sm.text-white-50
      //-   |  Generate Report
    // Content Row
    .row
      // Earnings (Monthly) Card Example
      .col-xl-3.col-md-6.mb-4
        .card.border-left-primary.shadow.h-100.py-2
          .card-body
            .row.no-gutters.align-items-center
              .col.mr-2
                .text-xs.font-weight-bold.text-primary.text-uppercase.mb-1 خەرجی
                |                       
                .h5.mb-0.font-weight-bold.text-gray-800="$"+0
              |                     
              .col-auto
                i.fas.fa-dollar-sign.fa-2x.text-gray-300
      // Earnings (Monthly) Card Example
      .col-xl-3.col-md-6.mb-4
        .card.border-left-success.shadow.h-100.py-2
          .card-body
            .row.no-gutters.align-items-center
              .col.mr-2
                .text-xs.font-weight-bold.text-success.text-uppercase.mb-1 داهات
                |                       
                .h5.mb-0.font-weight-bold.text-gray-800(id="Income")="$"+0
              |                     
              .col-auto
                i.fas.fa-dollar-sign.fa-2x.text-gray-300
      // Earnings (Monthly) Card Example
      .col-xl-3.col-md-6.mb-4
        .card.border-left-info.shadow.h-100.py-2
          .card-body
            .row.no-gutters.align-items-center
              .col.mr-2
                .text-xs.font-weight-bold.text-info.text-uppercase.mb-1 بەرهەمەکان
                |                       
                .row.no-gutters.align-items-center
                  .col-auto
                    .h5.mb-0.mr-3.font-weight-bold.text-gray-800=product.length
                  |                         
                  //- .col
                  //-   .progress.progress-sm.mr-2
                  //-     .progress-bar.bg-info(role='progressbar', style='width: 50%', aria-valuenow='50', aria-valuemin='0', aria-valuemax='100')
              |                     
              .col-auto
                i.fas.fa-coins.fa-2x.text-gray-300
      // Pending Requests Card Example
      .col-xl-3.col-md-6.mb-4
        .card.border-left-warning.shadow.h-100.py-2
          .card-body
            .row.no-gutters.align-items-center
              .col.mr-2
                .text-xs.font-weight-bold.text-warning.text-uppercase.mb-1 کڕیارەکان
                |                       
                .h5.mb-0.font-weight-bold.text-gray-800=profiles.length
              |                     
              .col-auto
                i.fas.fa-user-friends.fa-2x.text-gray-300
    // Content Row
    .row
      // Area Chart
      .col-xl-8.col-lg-7
        .card.shadow.mb-4
          // Card Header - Dropdown
          .card-header.py-3.d-flex.flex-row.align-items-center.justify-content-between
            h6.m-0.font-weight-bold.text-primary Earnings Overview
            |                   
            .dropdown.no-arrow
              a#dropdownMenuLink.dropdown-toggle(href='#', role='button', data-toggle='dropdown', aria-haspopup='true', aria-expanded='false')
                i.fas.fa-ellipsis-v.fa-sm.fa-fw.text-gray-400
              |                     
              .dropdown-menu.dropdown-menu-right.shadow.animated--fade-in(aria-labelledby='dropdownMenuLink')
                .dropdown-header Dropdown Header:
                |                       
                a.dropdown-item(href='#') Action
                |                       
                a.dropdown-item(href='#') Another action
                |                       
                .dropdown-divider
                |                       
                a.dropdown-item(href='#') Something else here
          // Card Body
          .card-body
            .chart-area
              canvas#myAreaChart
      // Pie Chart
      .col-xl-4.col-lg-5
        .card.shadow.mb-4
          // Card Header - Dropdown
          .card-header.py-3.d-flex.flex-row.align-items-center.justify-content-between
            h6.m-0.font-weight-bold.text-primary Revenue Sources
            |                   
            .dropdown.no-arrow
              a#dropdownMenuLink.dropdown-toggle(href='#', role='button', data-toggle='dropdown', aria-haspopup='true', aria-expanded='false')
                i.fas.fa-ellipsis-v.fa-sm.fa-fw.text-gray-400
              |                     
              .dropdown-menu.dropdown-menu-right.shadow.animated--fade-in(aria-labelledby='dropdownMenuLink')
                .dropdown-header Dropdown Header:
                |                       
                a.dropdown-item(href='#') Action
                |                       
                a.dropdown-item(href='#') Another action
                |                       
                .dropdown-divider
                |                       
                a.dropdown-item(href='#') Something else here
          // Card Body
          .card-body
            .chart-pie.pt-4.pb-2
              canvas#myPieChart
            |                   
            .mt-4.text-center.small
              span.mr-2
                i.fas.fa-circle.text-primary
                |  Muhsin
              |                     
              span.mr-2
                i.fas.fa-circle.text-success
                |  Farman
              |                     
              span.mr-2
                i.fas.fa-circle.text-info
                |  Hamza
    // Content Row
    //- .row
      // Content Column
      .col-lg-6.mb-4
        // Project Card Example
        .card.shadow.mb-4
          .card-header.py-3
            h6.m-0.font-weight-bold.text-primary زۆرترین بەرهەمی داواکراو
          |                 
          .card-body
              each item,i in mostwanted
                h4.small.font-weight-bold=item['itemName']+" "+item['color']
                  span.float-right=item['totalQuantity']
                |                   
                .progress.mb-4
                  .progress-bar.bg-primary(role='progressbar', style=`width:${item['totalQuantity']}%`, aria-valuenow=item['totalQuantity'], aria-valuemin='0', aria-valuemax=totalP)
      .col-lg-6.mb-4
        // Project Card Example
        .card.shadow.mb-4
          .card-header.py-3
            h6.m-0.font-weight-bold.text-primary زۆرترین بەرهەمی ماوە لەناو بارهەڵگر
          |                 
          .card-body
              each item,i in trailers
                h4.small.font-weight-bold=item['itemName']+"----"+item['trailerNumber']
                  span.float-right=item['totalQuantity']
                |                   
                .progress.mb-4
                  .progress-bar.bg-danger(role='progressbar', style=`width: ${item['totalQuantity']}%`, aria-valuenow='20', aria-valuemin='0', aria-valuemax='100')
                |                   
